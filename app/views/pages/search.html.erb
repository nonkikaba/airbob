<div id="main">
  <div id="left">
    <div class="row">
      <br>
      <div class="collapse" id="collapsePanel">
        <%= search_form_for @search, url: search_path do |f| %>
          <div class="row">
            <div class="col-md-8">
              <label>Price range:</label>
            </div>
            <div class="col-md-2">
              <label>Min Price</label>
              <%= f.text_field :price_gteq, class: "form-control" %>
            </div>
            <div class="col-md-2">
              <label>Min Price</label>
              <%= f.text_field :price_lteq, class: "form-control" %>
            </div>
          </div>
          <hr>

          <div class="row">
            <div class="col-md-3">
              <%= text_field_tag :start_date, params[:start_date], placeholder: 'where are you going?', class: 'form-control datepicker' %>
            </div>
            <div class="col-md-3">
              <%= text_field_tag :end_date, params[:end_date], placeholder: 'where are you going?', class: 'form-control datepicker' %>
            </div>
          </div>
        <% end %>
      </div>
    </div>
  </div>
  <div id="right"></div>
</div>
